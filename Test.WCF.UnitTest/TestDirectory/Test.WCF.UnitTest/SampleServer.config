<?xml version="1.0"?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="file" type="System.Diagnostics.TextWriterTraceListener" initializeData="SampleServer.log" traceOutputOptions="DateTime" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>